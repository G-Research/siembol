{
  "config_layout": {
    "$..parsing_applications.items": {
      "widget": {
        "formlyConfig": {
          "type": "tabs"
        }
      }
    },
    "$..parsing_app_description": {
      "widget": {
        "formlyConfig": {
          "type": "textarea"
        }
      }
    },
    "$..routing_parser": {
      "widget": {
        "formlyConfig": {
          "hideExpression": "field.parent.parent.model.parsing_app_settings.parsing_app_type !== 'router_parsing'"
        }
      }
    },
    "$..parsers": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "expansion-panel"
          ]
        }
      }
    },
    "$..parsers.items": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "panel"
          ]
        }
      }
    },
    "$..default_parser": {
      "wrappers": [
        "panel"
      ]
    },
    "$..single_parser": {
      "widget": {
        "formlyConfig": {
          "hideExpression": "field.parent.parent.model.parsing_app_settings.parsing_app_type !== 'single_parser'"
        }
      }
    },
    "$..metadata_prefix": {
      "widget": {
        "formlyConfig": {
          "hideExpression": "!model.parse_metadata"
        }
      }
    },
    "$..routing_field_pattern": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "form-field",
            "help-link"
          ],
          "templateOptions": {
            "link": "https://docs.oracle.com/javase/8/docs/api/java/util/regex/Pattern.html"
          }
        }
      }
    }
  },
  "admin_config_layout": {
    "$": {
      "widget": {
        "formlyConfig": {
          "type": "admin-tabs"
        }
      }
    },
    "$..['overridden.applications'].items": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "expansion-panel"
          ]
        }
      }
    },
    "$..['overridden.applications']..['kafka.spout.properties']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "help-link"
          ],
          "templateOptions": {
            "link": "https://kafka.apache.org/0102/documentation.html#consumerconfigs"
          }
        }
      }
    },
    "$..['overridden.applications']..['offset.commit.period.ms']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "form-field", "help-link"
          ],
          "templateOptions": {
            "link": "https://docs.cloudera.com/HDPDocuments/HDP3/HDP-3.1.5/storm-moving-data/content/tuning_kafkaspout_performance.html"
          }
        }
      }
    },
    "$..['overridden.applications']..['max.uncommitted.offsets']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "form-field", "help-link"
          ],
          "templateOptions": {
            "link": "https://docs.cloudera.com/HDPDocuments/HDP3/HDP-3.1.5/storm-moving-data/content/tuning_kafkaspout_performance.html"
          }
        }
      }
    },
    "$..['overridden.applications']..['storm.config']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "help-link"
          ],
          "templateOptions": {
            "link": "https://storm.apache.org/releases/current/Configuration.html"
          }
        }
      }
    },
    "$..['overridden.applications']..['producer.properties']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "help-link"
          ],
          "templateOptions": {
            "link": "https://kafka.apache.org/0102/documentation.html#producerconfigs"
          }
        }
      }
    },
    "$.properties.['storm.attributes']..['kafka.spout.properties']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "help-link"
          ],
          "templateOptions": {
            "link": "https://kafka.apache.org/0102/documentation.html#consumerconfigs"
          }
        }
      }
    },
    "$.properties.['storm.attributes']..['offset.commit.period.ms']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "form-field", "help-link"
          ],
          "templateOptions": {
            "link": "https://docs.cloudera.com/HDPDocuments/HDP3/HDP-3.1.5/storm-moving-data/content/tuning_kafkaspout_performance.html"
          }
        }
      }
    },
    "$.properties.['storm.attributes']..['max.uncommitted.offsets']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "form-field", "help-link"
          ],
          "templateOptions": {
            "link": "https://docs.cloudera.com/HDPDocuments/HDP3/HDP-3.1.5/storm-moving-data/content/tuning_kafkaspout_performance.html"
          }
        }
      }
    },
    "$.properties.['storm.attributes']..['storm.config']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "help-link"
          ],
          "templateOptions": {
            "link": "https://storm.apache.org/releases/current/Configuration.html"
          }
        }
      }
    },
    "$.properties.['kafka.batch.writer.attributes']..['producer.properties']": {
      "widget": {
        "formlyConfig": {
          "wrappers": [
            "help-link"
          ],
          "templateOptions": {
            "link": "https://kafka.apache.org/0102/documentation.html#producerconfigs"
          }
        }
      }
    }
  }
}